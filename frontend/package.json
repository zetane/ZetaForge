{
  "name": "zetaforge",
  "version": "0.2.0",
  "main": "dist-electron/main/index.js",
  "description": "ZetaForge",
  "author": "Zetane <info@zetane.com>",
  "license": "AGPL-3.0-only",
  "private": true,
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:all": "tsc && vite build && electron-builder --x64 --arm64 -mwl",
    "build:darwin-amd64": "tsc && vite build && electron-builder --mac --x64",
    "build:darwin-arm64": "tsc && vite build && electron-builder --mac --arm64",
    "build:windows": "tsc && vite build && electron-builder --win --x64",
    "build:linux-amd64": "tsc && vite build && electron-builder --linux --x64",
    "build:linux-arm64": "tsc && vite build && electron-builder --linux --arm64 --x64",
    "preview": "vite preview",
    "pree2e": "vite build --mode=test",
    "e2e": "playwright test",
    "lint": "eslint .",
    "format": "prettier --check ."
  },
  "dependencies": {
    "@sentry/electron": "^4.22.0",
